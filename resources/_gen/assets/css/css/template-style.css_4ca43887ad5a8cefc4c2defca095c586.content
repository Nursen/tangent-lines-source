.tl-container {
  max-width: 900px;
}
.tl-blog-post{
  max-width: 900px;
  width: 100%;
  margin: 0 auto;
  padding: 0;
}

.tl-header{
  background: url(//100.115.92.2:1313/img/header-bg-alt.jpg) no-repeat bottom;
  background-size: cover;
  height: 300px;
  position: relative;
}

.tl-nav{
  position: absolute;
  width: 100%;
  height: 50px;
  bottom: 0;
  background-color:rgba(0,0,0,0.65);
  color: white;
  text-transform: uppercase;
  padding: 0;
}


#nav a {
  color: inherit;
  font-size: x-large;
}
#nav-border {
  border-bottom: 1px solid #212529;
}
#main {
  margin-top: 1em;
  margin-bottom: 4em;
}
#home-jumbotron {
  background-color: inherit;
}
.font-125 {
  font-size: 125%;
}
.tag-btn {
  margin-bottom: 0.3em;
}
img {
  max-width: 100%;
}
